{
  "name": "Invert",
  "description": "Invert colors.",
  "version": "1.0",
  "manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["invert.js"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [ "invert.css"],
			"matches": ["<all_urls>"]
		}
	],
	"action": {
    "default_popup": "popup.html"
  },
	"permissions": [ "storage", "activeTab", "scripting" ]
}